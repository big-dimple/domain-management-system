# MongoDB配置
MONGO_USER=admin
MONGO_PASSWORD=domain123456
MONGODB_URI=mongodb://admin:domain123456@mongodb:27017/domains?authSource=admin

# 服务配置
NODE_ENV=production
PORT=3001
LOG_LEVEL=info

# CSV导入导出配置
CSV_MAX_SIZE=50MB
CSV_ENCODING=utf-8

# 域名扫描配置
SCAN_ENABLED=true
SCAN_CRON=30 4 * * *
SCAN_TIMEOUT=30000
SCAN_BATCH_SIZE=10

# SSL证书扫描配置
SSL_SCAN_ENABLED=true
SSL_SCAN_CRON=0 5 * * *
SSL_SCAN_TIMEOUT=20000
SSL_SCAN_BATCH_SIZE=3

# 告警配置
ALERT_ENABLED=true
ALERT_CRON=0 12 * * *

